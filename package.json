{
  "name": "react-js-template",
  "homepage": "",
  "version": "1.0.0",
  "description": "React JS Boilerplate Code",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server",
    "start": "npm run dev",
    "build": "cross-env NODE_ENV=production webpack",
    "clean": "rm -rf build && rm -rf node_modules && npm install",
    "lint:file": "cross-env eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint": "cross-env eslint . --ext .js,.jsx,.ts,.tsx --fix.",
    "pretty": "cross-env prettier --write '**/*.{j,t}{s,sx}'"
  },
  "keywords": [
    "react",
    "redux",
    "webpack"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.3.0",
    "@fortawesome/free-brands-svg-icons": "^6.0.0",
